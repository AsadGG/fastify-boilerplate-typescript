{
  "name": "fastify-boilerplate-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx index.ts",
    "start:dev": "tsx watch index.ts | pino-pretty",
    "log": "tsx print-log.ts",
    "format": "prettier ./src index.ts global-constants.ts --write",
    "lint": "eslint ./src index.ts global-constants.ts --fix",
    "prepare": "husky",
    "lint-staged": "lint-staged --concurrent false",
    "kysely-codegen": "kysely-codegen --camel-case",
    "migrate:make": "kysely migrate make",
    "migrate:list": "kysely migrate list",
    "migrate:latest": "kysely migrate latest && npm run kysely-codegen",
    "migrate:rollback": "kysely migrate rollback --all",
    "seed:run": "kysely seed run --specific=",
    "seed:list": "kysely seed list",
    "build": "tsc"
  },
  "imports": {
    "#configs/*": "./src/configs/*",
    "#plugins/*": "./src/plugins/*",
    "#repositories/*": "./src/repositories/*",
    "#utilities/*": "./src/utilities/*",
    "#src/*": "./src/*",
    "#root/*": "./*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/redis": "^7.0.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "github:AsadGG/fastify-swagger-ui",
    "@lukeed/ms": "^2.0.2",
    "@sinclair/typebox": "^0.34.33",
    "ajv": "^8.17.1",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "fastify-plugin": "^5.0.1",
    "kysely": "^0.28.2",
    "lodash": "^4.17.21",
    "pg": "^8.16.0",
    "pino": "^9.7.0",
    "pino-roll": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@inquirer/prompts": "^7.5.3",
    "@types/bcrypt": "^5.0.2",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.17.57",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "chalk": "^5.4.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "glob": "^11.0.2",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "kysely-codegen": "^0.18.5",
    "kysely-ctl": "^0.13.1",
    "lint-staged": "^16.1.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "pretty-bytes": "^7.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
