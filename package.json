{
  "name": "fastify-boilerplate-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx index.ts",
    "start:dev": "tsx watch index.ts | pino-pretty",
    "log": "tsx print-log.ts",
    "format": "prettier ./src index.ts global-constants.ts --write",
    "lint": "eslint ./src index.ts global-constants.ts --fix",
    "prepare": "husky",
    "lint-staged": "lint-staged --concurrent false",
    "kysely-codegen": "kysely-codegen --camel-case",
    "migrate:make": "kysely migrate make",
    "migrate:list": "kysely migrate list",
    "migrate:latest": "kysely migrate latest && npm run kysely-codegen",
    "migrate:rollback": "kysely migrate rollback --all",
    "seed:run": "kysely seed run --specific=",
    "seed:list": "kysely seed list",
    "build": "tsc && tsx post-build.ts",
    "check": "tsc --noEmit"
  },
  "imports": {
    "#configs/*": "./src/configs/*.js",
    "#plugins/*": "./src/plugins/*.js",
    "#repositories/*": "./src/repositories/*.js",
    "#utilities/*": "./src/utilities/*.js",
    "#src/*": "./src/*.js",
    "#root/*": "./*.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/redis": "^7.0.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "github:AsadGG/fastify-swagger-ui",
    "@lukeed/ms": "^2.0.2",
    "@sinclair/typebox": "^0.34.41",
    "ajv": "^8.17.1",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1",
    "kysely": "^0.28.5",
    "lodash": "^4.17.21",
    "pg": "^8.16.3",
    "pino": "^9.9.2",
    "pino-pretty": "^13.1.1",
    "pino-roll": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@inquirer/prompts": "^7.8.4",
    "@types/bcrypt": "^6.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.13",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "chalk": "^5.6.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.18.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "pretty-bytes": "^7.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
