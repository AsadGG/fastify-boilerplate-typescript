{
  "name": "fastify-boilerplate-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx index.ts",
    "start:dev": "tsx watch index.ts | pino-pretty",
    "log": "tsx print-log.ts",
    "format": "prettier ./src index.ts global-constants.ts --write",
    "lint": "eslint ./src index.ts global-constants.ts --fix",
    "prepare": "husky",
    "lint-staged": "lint-staged --concurrent false",
    "kysely-codegen": "kysely-codegen --camel-case",
    "migrate:make": "tsx ./src/db/migrate-make.ts --",
    "migrate:list": "tsx ./src/db/migrate-list.ts",
    "migrate:latest": "tsx ./src/db/migrate-latest.ts && npm run kysely-codegen",
    "migrate:rollback": "tsx ./src/db/migrate-rollback.ts --",
    "seed:list": "tsx ./src/db/seed-list.ts",
    "seed:run": "tsx ./src/db/seed-run.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.10.0",
    "@fastify/cors": "^8.0.1",
    "@fastify/env": "^4.4.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.1",
    "@lukeed/ms": "^2.0.2",
    "@sinclair/typebox": "^0.33.15",
    "ajv": "^8.17.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-bcrypt": "^1.0.1",
    "fastify-plugin": "^4.5.1",
    "kysely": "^0.27.4",
    "lodash": "^4.17.21",
    "pg": "^8.13.0",
    "pino": "^8.21.0",
    "sonic-boom": "^3.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@inquirer/prompts": "^3.3.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.10",
    "@types/node": "^20.16.11",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chalk": "^5.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "glob": "^10.4.5",
    "husky": "^8.0.3",
    "kysely-codegen": "^0.14.2",
    "lint-staged": "^15.2.10",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.3.3",
    "pretty-bytes": "^6.1.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
