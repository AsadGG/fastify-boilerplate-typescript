{
  "name": "fastify-boilerplate-typescript",
  "type": "module",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "imports": {
    "#configs/*": "./src/configs/*.js",
    "#plugins/*": "./src/plugins/*.js",
    "#repositories/*": "./src/repositories/*.js",
    "#schemas/*": "./src/schemas/*.js",
    "#utilities/*": "./src/utilities/*.js",
    "#src/*": "./src/*.js",
    "#root/*": "./*.js"
  },
  "main": "index.js",
  "scripts": {
    "start": "tsx index.ts",
    "start:dev": "tsx watch index.ts",
    "log": "tsx print-log.ts",
    "kysely-codegen": "kysely-codegen --camel-case --out-file src/types/database.ts",
    "migrate:make": "kysely migrate make",
    "migrate:list": "kysely migrate list",
    "migrate:latest": "kysely migrate latest && npm run kysely-codegen",
    "migrate:rollback": "kysely migrate rollback --all",
    "seed:run": "kysely seed run --specific=",
    "seed:list": "kysely seed list",
    "build": "tsc && tsx post-build.ts",
    "check": "tsc --noEmit",
    "lint:fix": "eslint . --fix --cache",
    "lint-staged": "lint-staged",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/error": "^4.2.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/sensible": "^6.0.4",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "github:AsadGG/fastify-swagger-ui",
    "@lukeed/ms": "^2.0.2",
    "@sinclair/typebox": "^0.34.41",
    "ajv": "^8.17.1",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint-plugin-unicorn": "^62.0.0",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "kysely": "^0.28.8",
    "lodash": "^4.17.21",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "pino-roll": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.3.0",
    "@inquirer/prompts": "^8.0.2",
    "@types/bcrypt": "^6.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^22.19.1",
    "@types/pg": "^8.15.6",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.19.0",
    "lint-staged": "^16.2.7",
    "pretty-bytes": "^7.1.0",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint-staged && npm run check"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
